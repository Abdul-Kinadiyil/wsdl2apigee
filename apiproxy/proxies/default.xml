<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <Description>SOAP proxy generated from undefined</Description>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request/>
        <Response/>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request>
            <Step>
                <Name>remove-empty-nodes</Name>
            </Step>
        </Request>
        <Response/>
    </PostFlow>
    <Flows>
<Flow name="process">
<Description>process</Description>
<Request>
<Step>
<Name>json-to-xml</Name>
</Step>
<Step>
<Name>process-add-namespace</Name>
</Step>
<Step>
<Name>process-add-other-namespaces</Name>
</Step>
<Step>
<Name>add-soap11</Name>
</Step>
</Request>
<Response/>
<Condition>(proxy.pathsuffix MatchesPath "/process") and (request.verb = "POST")</Condition>
</Flow>
<Flow name="unknown-resource">
<Description>Unknown Resource</Description>
<Request>
<Step>
<Name>unknown-resource</Name>
</Step>
</Request>
<Response/>
<Condition/>
</Flow>
</Flows>
    <HTTPProxyConnection>
        <BasePath>/foxedfeventproducer_bpel_client_ep</BasePath>
        <Properties/>
        <VirtualHost>default</VirtualHost>
    </HTTPProxyConnection>
    <RouteRule name="default">
        <TargetEndpoint>default</TargetEndpoint>
    </RouteRule>
</ProxyEndpoint>
